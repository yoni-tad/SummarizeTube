{
    "manifest_version": 3,
    "name": "SummarizeTube",
    "version": "1.0.0",
    "description": "Summarizes & explains YouTube content.",
    "background": {
        "service_worker": "service-worker.js"
    },
    ""
    "permissions": ["sidePanel", "tabs"],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Y",
                "mac": "Command+Y"
            }
        }
    },
    "action": {
        "default_icon": "assets/logo.png"
    }
}